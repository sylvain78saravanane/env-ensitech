{
    "source": {
        "include": ["src", "README.md"],
        "exclude": ["node_modules", "tests"],
        "includePattern": ".*\\.js$",
        "excludePattern": "(node_modules|tests)"
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "plugins": []
}

